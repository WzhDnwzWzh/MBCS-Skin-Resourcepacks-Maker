{
  "name": "mbcs-skin-resourcepacks-maker",
  "version": "1.0.0",
  "description": "It is a program that can make Minecraft's resourcepacks with a player's skin",
  "main": "src/main.js",
  "build": {
    "productName": "mbcs-skin-resourcepacks-maker",
    "appId": "com.mbcs-skin-resourcepacks-maker.app",
    "win": {
      "icon": "image/icon.ico",
      "target": [
        "zip"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WzhDnwzWzh/mbcs-skin-resourcepacks-maker"
  },
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder --win"
  },
  "keywords": [
    "minecraft",
    "skin",
    "mbcs",
    "resourcepacks",
    "maker"
  ],
  "author": "Minecraft Byte Cube Studio",
  "license": "MIT",
  "devDependencies": {
    "electron": "^6.0.0"
  }
}
